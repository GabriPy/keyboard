{
    "name": "component-keyboard-copy",
    "version": "1.0.0",
    "description": "Scene exported with Spline",
    "scripts": {
        "start": "serve",
        "copy-html": "copyfiles \"./src/**/*.html\" ./dist",
        "copy-css": "copyfiles \"./src/**/*.css\" ./dist",
        "copy-js": "copyfiles \"./src/**/*.js\" ./dist",
        "build": "npm-run-all --parallel copy-html copy-css copy-js"
    },
    "devDependencies": {
        "copyfiles": "^2.4.1",
        "npm-run-all": "^4.1.5",
        "serve": "^13.0.2"
    },
    "dependencies": {
        "build": "^0.1.4"
    }
}
